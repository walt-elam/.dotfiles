<?xml version="1.0" encoding="UTF-8"?>

<!-- OpenBox menu configuration file -->

<openbox_menu
    xmlns="http://openbox.org/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://openbox.org/
        file:///usr/share/openbox/menu.xsd">

<menu id="root-menu" label="Openbox">
    <item label="Terminal">
        <action name="Execute">
            <execute>/usr/bin/xterm</execute>
        </action>
    </item>
    <item label="Web Browser">
        <action name="Execute">
            <execute>/etc/alternatives/www-browser</execute>
        </action>
    </item>

    <separator />

    <menu id="applications-menu" label="Applications" 
        execute="/usr/bin/obamenu" />

    <separator />

    <item label="Reconfigure"><action name="Reconfigure" /></item>
    <item label="Restart"><action name="Restart" /></item>
    <item label="Exit"><action name="Exit" /></item>
</menu>

</openbox_menu>

