!
!   Xresources
!
!   X client application settings
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

! xterm
!!!!!!!

! Application Resources
XTerm.maxBufSize: 65536
XTerm.minBufSize: 1024
XTerm.termName: xterm-256color
XTerm.title: console

! VT100 Widget Resources
XTerm.vt100.allowBoldFonts: true
XTerm.vt100.allowFontOps: true
XTerm.vt100.allowScrollLock: true
XTerm.vt100.allowBoldMode: true
XTerm.vt100.background: XtDefaultBackground
XTerm.vt100.ctlXtraScroll: true
XTerm.vt100.color0: black
XTerm.vt100.color1: red3
XTerm.vt100.color2: green3
XTerm.vt100.color3: yellow3
XTerm.vt100.color4: dark blue
XTerm.vt100.color5: magenta3
XTerm.vt100.color6: cyan3
XTerm.vt100.color7: gray90
XTerm.vt100.color8: gray30
XTerm.vt100.color9: red
XTerm.vt100.color10: green
XTerm.vt100.color11: yellow
XTerm.vt100.color12: light blue
XTerm.vt100.color13: magenta
XTerm.vt100.color14: cyan
XTerm.vt100.color15: white
XTerm.vt100.cursorBlink: true
Xterm.vt100.cursorColor: orange3
XTerm.vt100.cursorOnTime: 500
XTerm.vt100.cursorOffTime: 500
XTerm.vt100.cutNewline: false
XTerm.vt100.cutToBeginningOfLine: false
XTerm.vt100.defaultString: .
XTerm.vt100.dynamicColors: true
XTerm.vt100.font: fixed
XTerm.vt100.foreground: XtDefaultForeground
XTerm.vt100.internalBorder: 0
XTerm.vt100.on2Clicks: word
XTerm.vt100.on3Clicks: line
XTerm.vt100.on4Clicks: page
XTerm.vt100.on5Clicks: none
XTerm.vt100.rightScrollBar: true
XTerm.vt100.scrollBar: true
XTerm.vt100.scrollBarBorder: 2
XTerm.vt100.scrollKey: true
XTerm.vt100.trimSelection: true
XTerm.vt100.utf8: true
XTerm.vt100.utf8Title: true

! Scrollbar Resources
XTerm*Scrollbar.Thickness: 12

! Other Resources
xterm*font: *-fixed-*-*-*-18-*

